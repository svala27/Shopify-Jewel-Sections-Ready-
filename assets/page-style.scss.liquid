        }
      }
    }

    .panel-group {
      margin-bottom: 35px;
      word-break: break-word;
      .col-pre-sales-questions{
        margin-bottom: 46px;
      }
      .header-title{
        .title{
          font-size: $font-size + 6;
          font-weight: 600;
          letter-spacing: .04em;
          line-height: 24px;
          color: #8b714a;
          padding: 20px 0px 19px;
          border-bottom: 1px solid #f2e3ca;
          font-family: 'DM Serif Display', serif;
          text-transform: capitalize;
          margin-bottom: 1px;
        }
      }
    }
    .panel-title a,
    .panel-body {
      padding: 9px 90px 11px 0px;
      @include flexbox();
      @include align-items(flex-start);

      div:before {
        display: inline-block;
        font-size: $font_size;
        line-height: 20px;
        padding: 0px 8px 0px;
        letter-spacing: .02em;
        min-width: 61px;
        @include border-radius(1px);
        margin-right: 20px;
        text-align: center;
      }
      &> div .rte{
        display: inline-block;
        vertical-align: top;
      }
    }
    .panel-body{
      .rte{
        letter-spacing: .04em;
        line-height: 24px;
      }
    }
    .panel-title {
      border-bottom: 1px solid #ececec;
      margin-bottom: 0px;
      padding: 11px 0px 10px;

      a {
        position: relative;
        font-size: $font_size;
        line-height: $font_size + 12;
        font-weight: 500;
        margin-bottom: 0;
        letter-spacing: .04em;
        &:before{
           content: "";
          position: absolute;
          right: 0;
          top: 50%;
          -moz-transform: translateY(-50%);
          -o-transform: translateY(-50%);
          -ms-transform: translateY(-50%);
          -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
          width: 16px;
          height: 2px;
          background: #c8c8c8;
        }
        &:after {
            content: "";
            position: absolute;
            right: 7px;
            top: 50%;
            -moz-transform: translateY(-50%);
            -o-transform: translateY(-50%);
            -ms-transform: translateY(-50%);
            -webkit-transform: translateY(-50%);
            transform: translateY(-50%);
            width: 2px;
            height: 16px;
            background: #c8c8c8;
        }

        &:not(.collapsed) {
          &:after {
            display: none;
          }
        }
      }
    }

    .panel-body {
      padding-top: 26px;
      padding-bottom: 10px;
      line-height: $font_size + 10;

      &>div:before {
        position: relative;
        top: 0px;
      }

      p {
        &:not(:last-child),
        &:not(:last-of-type) {
          margin-bottom: 18px;
        }
      }
    }
    .contact-link {
      @include flexbox();
      @include justify-content(flex-end);
      @include align-items(center);
      margin: 55px 0px 125px;

      .text {
        font-size: $font_size;
        line-height: $font_size + 12;
        margin-right: 30px;
        letter-spacing:.04em;
      }

      .btn {
        max-width: 270px;
        font-size:$font-size;
        border-color: transparent;;
        color: #fff;
        background: #f4a884;

        &:hover {
           border-color: #f4a884;;
          color: #f4a884;
          background: transparent;
        }
      }
    }
    .sidebar-faqs{
      -webkit-box-flex: 0;
        -ms-flex: 0 0 270px;
        flex: 0 0 270px;
        max-width: 270px;
        .close-sidebar{
          display: none;
          @media (max-width: 1199px){
            display: block;
          }
        }
      .need-help-sidebar{
        background: #fafafa;
        padding: 5px 30px 45px 30px;
        h3{
          font-size: 18px;
          font-weight: 600;
          letter-spacing: .04em;
          line-height: 24px;
          color: #8b714a;
          padding: 14px 0px 19px;
          border-bottom: 1px solid #f2e3ca;
          font-family: 'DM Serif Display', serif;
          text-transform: capitalize;
          text-align: center;
          margin-bottom: 27px;
      }
      .need-help{
        @include flexbox();
        @include align-items(flex-end);
        @include justify-content(space-between);
          a{
              color: #787878;
              text-align: center;
              line-height: 24px;
              font-weight: 500;
              font-size: $font-size;
              letter-spacing: .04em;
              text-transform: uppercase;
              width: 50%;
              padding: 0 12px;
              i{
                  font-size: 28px;
                  color: #5a5a5a;
              }
          }
      }
      }
    }
  }
  
